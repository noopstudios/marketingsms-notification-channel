{
    "name": "noopstudios/marketingsms-notification-channel",
    "description": "MarketingSMS Notifications Channel for Laravel",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.5",
        "noopstudios/marketingsms-php": "^1.1.5",
        "illuminate/notifications": "^9.0|^10.3|^11.0|^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NotificationChannels\\MarketingSMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\MarketingSMS\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Noop",
            "email": "dev@noop.pt"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.4",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\MarketingSMS\\MarketingSMSProvider"
            ]
        }
    }
}
